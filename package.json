{
  "name": "test-auth-service",
  "version": "1.0.0",
  "description": "AirAsia Assessment: Basic auth flow service with roles and permissions.",
  "main": "app.js",
  "scripts": {
    "test": "npm run db:migrate:undo:all && npm run db:migrate && mocha --timeout 10000",
    "start:prod": "NODE_ENV=production node app.js",
    "start:local": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed:all && npm run nodemon",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "nodemon": "nodemon app.js"
  },
  "author": "Masih Niaz",
  "license": "ISC",
  "homepage": "https://github.com/masihniaz/test-auth-service#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masihniaz/test-auth-service.git"
  },
  "bugs": {
    "url": "https://github.com/masihniaz/test-auth-service/issues"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.0.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sequelize": "^6.6.5",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "npm": ">=7.20.0",
    "node": ">=15.8.0"
  }
}
